{
	"name": "sayuna",
	"version": "2.5.4",
	"description": "Easily extensible and customizable all-in-one Discord bot. Moderation, music & fun!",
	"author": "Witold Zawada",
	"private": true,
	"license": "MIT",
	"type": "module",
	"main": "build/main.js",
	"engines": {
		"node": ">=24.x.x",
		"pnpm": ">=10.x.x"
	},
	"scripts": {
		"start": "node --env-file .env build/main.js",
		"start:prod": "NODE_ENV='production' pm2 start build/main.js --name sayuna",
		"dev": "NODE_ENV='development' node --env-file .env --watch --import tsx src/main.ts",
		"build": "tsc",
		"check": "pnpm biome check --fix ./src"
	},
	"dependencies": {
		"@discordjs/voice": "^0.19.0",
		"@discordx/importer": "^1.3.3",
		"@discordx/pagination": "^4.4.0",
		"@discordx/utilities": "^8.0.0",
		"@distube/deezer": "^2.0.1",
		"@distube/direct-link": "^1.0.1",
		"@distube/soundcloud": "^2.0.4",
		"@distube/spotify": "^2.0.2",
		"@distube/youtube": "^1.0.4",
		"@distube/yt-dlp": "^2.0.1",
		"@distube/ytsr": "^2.0.4",
		"discord.js": "^14.25.1",
		"discordx": "^11.13.3",
		"distube": "^5.2.3",
		"libsodium-wrappers": "^0.8.2",
		"moment": "^2.30.1",
		"opusscript": "^0.1.1",
		"reflect-metadata": "^0.2.2",
		"sodium-native": "^5.0.10",
		"winston": "^3.19.0",
		"winston-transport": "^4.9.0",
		"youtubei.js": "^16.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.3",
		"@swc/core": "^1.15.11",
		"@types/node": "^25.3.0",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@distube/yt-dlp",
			"@swc/core",
			"esbuild"
		]
	},
	"volta": {
		"node": "24.13.1",
		"pnpm": "10.30.1"
	}
}
