{
	"name": "sayuna",
	"version": "2.3.2",
	"description": "Easily extensible and customizable all-in-one Discord bot. Moderation, music & fun!",
	"author": "Witold Zawada",
	"private": true,
	"license": "MIT",
	"type": "module",
	"main": "build/main.js",
	"engines": {
		"node": ">=22.x.x"
	},
	"scripts": {
		"start": "node build/main.js",
		"start:prod": "NODE_ENV='production' pm2 start build/main.js --name sayuna",
		"dev": "NODE_ENV='development' node --watch --import tsx src/main.ts",
		"build": "tsc",
		"check": "pnpm biome check --fix --unsafe ./src"
	},
	"dependencies": {
		"@discordjs/voice": "^0.18.0",
		"@discordx/importer": "^1.3.1",
		"@discordx/pagination": "^3.5.6",
		"@discordx/utilities": "^7.0.3",
		"@distube/deezer": "^2.0.1",
		"@distube/direct-link": "^1.0.1",
		"@distube/soundcloud": "^2.0.4",
		"@distube/spotify": "^2.0.2",
		"@distube/youtube": "^1.0.4",
		"@distube/yt-dlp": "^2.0.1",
		"@distube/ytdl-core": "^4.16.5",
		"@distube/ytsr": "^2.0.4",
		"chatgpt": "^5.2.5",
		"discord.js": "^14.18.0",
		"discordx": "^11.12.2",
		"distube": "^5.0.6",
		"libsodium-wrappers": "^0.7.15",
		"moment": "^2.30.1",
		"opusscript": "^0.1.1",
		"reflect-metadata": "^0.2.2",
		"sodium-native": "^4.3.3",
		"winston": "^3.17.0",
		"winston-transport": "^4.9.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@swc/core": "^1.11.11",
		"@types/node": "^22.13.11",
		"dotenv": "^16.4.7",
		"tsx": "^4.19.3",
		"typescript": "5.8.2"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@distube/yt-dlp",
			"@swc/core",
			"esbuild"
		]
	}
}
