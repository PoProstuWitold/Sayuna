		"chatgpt": "^5.2.5",
		"discord.js": "^14.15.3",
		"discordx": "^11.11.2",
		"distube": "^4.2.2",
		"libsodium-wrappers": "^0.7.13",
		"moment": "^2.30.1",
		"opusscript": "^0.1.1",
		"reflect-metadata": "^0.2.2",
		"ts-node": "^10.9.2",
