{
	"name": "sayuna",
	"version": "2.2.0",
	"description": "Easily extensible and customizable all-in-one Discord bot. Moderation, music & fun!",
	"author": "Witold Zawada",
	"private": true,
	"license": "MIT",
	"type": "module",
	"main": "build/main.js",
	"engines": {
		"node": ">=20.x.x"
	},
	"scripts": {
		"start": "node build/main.js",
		"start:prod": "NODE_ENV='production' pm2 start build/main.js --name sayuna",
		"dev": "NODE_ENV='development' node --watch --import tsx src/main.ts",
		"build": "tsc",
		"check": "pnpm biome check --write ./src"
	},
	"dependencies": {
		"@discordjs/voice": "^0.18.0",
		"@discordx/importer": "^1.3.1",
		"@discordx/pagination": "^3.5.5",
		"@discordx/utilities": "^7.0.3",
		"@distube/deezer": "^2.0.1",
		"@distube/direct-link": "^1.0.1",
		"@distube/soundcloud": "^2.0.4",
		"@distube/spotify": "^2.0.2",
		"@distube/youtube": "^1.0.4",
		"@distube/yt-dlp": "^2.0.1",
		"@distube/ytdl-core": "^4.15.1",
		"@distube/ytsr": "^2.0.4",
		"chatgpt": "^5.2.5",
		"discord.js": "^14.16.3",
		"discordx": "^11.12.2",
		"distube": "^5.0.4",
		"moment": "^2.30.1",
		"opusscript": "^0.1.1",
		"reflect-metadata": "^0.2.2",
		"sodium-native": "^4.3.1",
		"winston": "^3.17.0",
		"winston-transport": "^4.9.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@swc/core": "^1.9.2",
		"@types/node": "^22.9.1",
		"dotenv": "^16.4.5",
		"tsx": "^4.19.2",
		"typescript": "5.6.3"
	}
}
